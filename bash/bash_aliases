alias psg='ps -ef | grep'
alias postsizes='echo '\''select t1.datname AS db_name, pg_size_pretty(pg_database_size(t1.datname)) as db_size from pg_database t1 order by pg_database_size(t1.datname) desc;'\'' | psql'
